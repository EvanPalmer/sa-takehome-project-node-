<main>
  <div class="mt-40 text-center text-success">
    <h1>
      <i class="far fa-check-circle"></i> 
      Success!
    </h1>
  </div>
  <div class="mt-40 text-center">
    <div class="mt-20 text-center text-secondary border-placeholder">
      <div>
        Thank you {{session.customer_details.name}}
      </div>
      <div>
        {{session.payment_status}} {{session.amount_total}}
      </div>
      <div>
      </div>
      <table>
      {{#each lineItems.data}}
      <tr>
        <td>
          {{description}} 
        </td>
        <td>
          {{price.unit_amount}}
        </td>
      </tr>
      {{/each}}
      </table>
    </div>
  </div>
</main>